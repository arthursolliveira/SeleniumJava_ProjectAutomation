package br.so.soliveira.pages;

import br.so.soliveira.core.BasePage;

public class MenuPage extends BasePage{
	
	public void acessarTelaInserirConta() {
		clicarLink("Contas");
		clicarLink("Adicionar");
	}
	
	public void acessarTelalistarConta() {
		clicarLink("Contas");
		clicarLink("Listar");
	}

}
